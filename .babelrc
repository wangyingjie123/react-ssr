{
  "presets": [
    ["@babel/preset-env", { "modules": false }],
    "@babel/preset-react"
  ],
  "plugins": [
    "@loadable/babel-plugin",
    ["import", {
      "libraryName": "antd",
      "libraryDirectory": "es",
